{
  "name": "earwego",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npx react-snap",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "reactSnap": {
    "source": "dist",
    "headless": true,
    "skipThirdPartyRequests": true,
    "minifyHtml": false,
    "inlineCss": false,
    "crawl": true,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-gpu",
      "--disable-dev-shm-usage"
    ],
    "routes": [
      "/",
      "/Earwaxremovalpage",
      "/symptoms",
      "/terms-conditions",
      "/privacy-policy",
      "/earwax-removal-nottingham",
      "/404.html"
    ]
  },
  "dependencies": {
    "@emailjs/browser": "^4.1.0",
    "@tailwindcss/vite": "^4.1.11",
    "aos": "^2.3.4",
    "emailjs-com": "^3.2.0",
    "glob": "^11.0.3",
    "lucide-react": "^0.539.0",
    "mkdirp": "^3.0.1",
    "puppeteer": "^24.29.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-router-dom": "6.14.2",
    "react-router-hash-link": "^2.4.3",
    "react-slick": "^0.31.0",
    "react-snap": "^1.23.0",
    "rimraf": "^6.1.0",
    "slick-carousel": "^1.8.1",
    "swiper": "^12.0.3",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.0.4"
  }
}
